      },
      "googleServicesFile": "./google-services.json",
      "package": "com.tarcioswin.ArrobaTrends"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5255381f-2996-4d45-a871-b3db09117c0f"
      }
