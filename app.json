      "@react-native-google-signin/google-signin"
    ],
    "slug": "ArrobaTrends",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FigureApp.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/FigureApp.jpeg",
